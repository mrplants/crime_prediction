{
    "name": "Getting Started Example",
    "version": "1.0",
    "description": "Build an Extension!",
    "permissions": ["storage", "tabs", "activeTab","<all_urls>"],
	 "content_scripts": [
    {
     "matches": ["<all_urls>"],
     "js": ["contentScript.js"]
    }
    ]  ,
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "manifest_version": 2 
}